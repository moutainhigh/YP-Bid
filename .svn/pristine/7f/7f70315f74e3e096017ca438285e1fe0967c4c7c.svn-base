 * @Author: ZhongShengbin
 * @Date: 2020/05/30 14:29
 * Copyright (c) 2019, Samton. All rights reserved
 */
@Controller
@RequestMapping("/manage/planproject/")
public class PlanDatasourceController extends BaseController {


    /**
     * 服务对象
     */
    @Resource
    private IPlanDatasourceService planDatasourceService;

    @Resource
    private ILogService logService;

    @RequestMapping("datasource/queryDatasourceType" + WebConstant.PAGE_SUFFIX)
    public String queryDatasourceList() throws Exception {
        return "plan/dataSourceTypeList";
    }

    @RequestMapping("datasource/queryDatasourceList" + WebConstant.NO_AUTH_SUFFIX)
    public String queryDatasourcePageList(Pagination<PlanDatasourceVo> paramBean, PlanDatasourceVo planDatasourceVo) throws Exception {
        paramBean.setSearch(planDatasourceVo);
        Pagination<PlanDatasourceVo> pageData = planDatasourceService.queryPlanDatasourceList(paramBean);
        this.addAttr("pageData", pageData);
        return "plan/dataSourceList";
    }

    @RequestMapping("datasource/exportDatasourceList" + WebConstant.BUSINESS_SUFFIX)
    public String exportDatasourceList(Pagination<PlanDatasourceVo> paramBean, PlanDatasourceVo planDatasourceVo) throws Exception {
        paramBean.setSearch(planDatasourceVo);
        Pagination<PlanDatasourceEntity> list = planDatasourceService.exportPlanDatasourceList(paramBean);
        logService.addLog(new TSysLog("拟建项目来源-导出", "导出拟建项目来源！", PmStateConstant.LOG_PLATFORM));
        this.export(response, "拟建项目来源设置" + String.format("%1$tY%1$tm%1$td", new Date()), PlanDatasourceEntity.class, list.getData());
        return null;
    }


    @RequestMapping("datasource/viewDatasource" + WebConstant.BUSINESS_SUFFIX)
    @ResponseBody
    public Map<String, Object> viewPlanDatasource(Integer datasourceId) throws Exception {
        PlanDatasourceVo planDatasourceVo = planDatasourceService.queryPlanDatasourceListById(datasourceId);
        return this.getResultMap(true, planDatasourceVo);
    }

