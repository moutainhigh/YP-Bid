    @RequestMapping("datasource/delDatasource" + WebConstant.BUSINESS_SUFFIX)
    @ResponseBody
    public Map<String, Object> delDatasource(@RequestParam(value = "datasourceId") Integer datasourceId) throws Exception {
        //校验主键ID
        if (datasourceId == null) {
            throw new ServiceException(PlanProjectExpCodeConstant.PLAN_SOURCE_DEL_ERROR);
        }
        //执行删除
        boolean result = planDatasourceService.deleteById(datasourceId);
        if (result) {
            logService.addLog(new TSysLog("拟建项目来源设置-删除", "删除拟建项目来源 ！", PmStateConstant.LOG_PLATFORM));
        } else {
            logService.addLog(new TSysLog("拟建项目来源设置-删除", "删除拟建项目来源！", PmStateConstant.LOG_PLATFORM));
        }
        if (!result) {
            throw new ServiceException(PlanProjectExpCodeConstant.PLAN_SOURCE_DEL_ERROR);
        }
        return this.getResultMap(true, result);
    }

    /**
     * @return {@link Map< String, Object>}
     * @author zsb
     * @date 2020/5/22 9:23
     */
    @RequestMapping("datasource/bidDatasourceList" + WebConstant.BUSINESS_SUFFIX)
    @ResponseBody
    public Map<String, Object> disAndEnableDatasource(PlanDatasourceVo datasourceVo) throws Exception {
        //校验主键ID
        datasourceVo.setUpdateUserId(CurrentUtil.getCurrentUser().getUserId().intValue());
        datasourceVo.setUpdateDate(new Date());
        if (datasourceVo.getDatasourceId() == null) {
            throw new ServiceException(DatasourceTypeExpCodeConstant.DATASOURCE_SET_ERROR);
        }
        //执行编辑
        boolean result = planDatasourceService.updateSelectiveById(datasourceVo);
        String headStr = datasourceVo.getIsForbid() == 0 ? "启用" : "禁用";
        if (result) {
            logService.addLog(new TSysLog("拟建项目来源管理-禁用/启用", headStr + "站点【" + datasourceVo.getDatasourceWebname() + "】成功 ！", PmStateConstant.LOG_PLATFORM));
        } else {
            logService.addLog(new TSysLog("拟建项目来源管理-禁用/启用", headStr + "站点【" + datasourceVo.getDatasourceWebname() + "】失败！", PmStateConstant.LOG_PLATFORM));
        }
        if (!result) {
            throw new ServiceException(DatasourceTypeExpCodeConstant.DATASOURCE_SET_ERROR);
        }
        return this.getResultMap(true, result);
    }
}
